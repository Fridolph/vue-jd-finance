{
  "env": {
    "browser": true,
    "node": true,
    "commonjs": true,
    "amd": true,
    "es6": true,
    "mocha": true
  },
  "parserOptions": {
    "ecmaVersion": 7,
    "sourceType": "module",
    "ecmaFeatures": {
      "globalReturn": true,
      "impliedStrict": true,
      "jsx": true,
      "destructuring": true,
      "generators": true,
      "superInFunctions": true,
      "experimentalObjectRestSpread": true
    }
  },
  "parser": "babel-eslint",
  "rules": {
    "no-underscore-dangle": 0,
    "generator-star-spacing": 0,
    "arrow-parens": 0,
    "no-caller": 1,
    "no-class-assign": 2,
    "no-cond-assign": 2,
    "no-const-assign": 2,
    "no-constant-condition": 2,
    "no-duplicate-case": 2,
    "no-empty": 2,
    "no-empty-character-class": 2,
    "no-eval": 1,
    "no-ex-assign": 2,
    "no-extend-native": 2,
    "no-extra-bind": 2,
    "no-extra-boolean-cast": 2,
    "no-extra-parens": 2,
    "no-extra-semi": 2,
    "no-fallthrough": 1,
    "no-floating-decimal": 2,
    "no-func-assign": 2,
    "no-implied-eval": 2,
    "no-inline-comments": 0,
    "no-inner-declarations": [2, "functions"],
    "no-invalid-regexp": 2,
    "no-invalid-this": 2,
    "no-irregular-whitespace": 2,
    "no-iterator": 2,
    "no-label-var": 2,
    "no-labels": 2,
    "no-lone-blocks": 2,
    "no-lonely-if": 2,
    "no-loop-func": 1,
    "no-mixed-requires": [0, false],
    "no-mixed-spaces-and-tabs": [2, false],
    "linebreak-style": [0, "windows"],
    "no-multi-spaces": 1,
    "no-multi-str": 2,
    "no-multiple-empty-lines": [1, { "max": 2 }],
    "no-native-reassign": 2,
    "no-negated-in-lhs": 2,
    "no-nested-ternary": 0,
    "no-new": 1,
    "no-new-func": 1,
    "no-new-object": 2,
    "no-new-require": 2,
    "no-new-wrappers": 2,
    "no-obj-calls": 2,
    "no-octal": 2,
    "no-octal-escape": 2,
    "no-param-reassign": 0,
    "no-redeclare": 2,
    "no-regex-spaces": 2,
    "no-restricted-modules": 0,
    "no-return-assign": 1,
    "no-sequences": 0,
    "no-spaced-func": 2,
    "no-trailing-spaces": 1,
    "no-this-before-super": 0,
    "no-throw-literal": 2,
    "no-undef": 0,
    "no-undef-init": 2,
    "no-undefined": 0,
    "no-unexpected-multiline": 2,
    "no-unneeded-ternary": 2,
    "no-unreachable": 2,
    "no-useless-call": 2,
    "no-void": 2,
    "no-var": 0,
    "no-with": 2,
    "array-bracket-spacing": [2, "never"],
    "arrow-spacing": 0,
    "callback-return": 1,
    "camelcase": 0,
    "comma-dangle": [2, "never"],
    "constructor-super": 0,
    "curly": [2, "all"],
    "default-case": 2,
    "dot-location": 0,
    "indent": [2, 2],
    "key-spacing": [0, { "beforeColon": false, "afterColon": true }],
    "quotes": [1, "single"],
    "radix": 2,
    "id-match": 0,
    "semi": 0,
    "space-before-function-paren": [0, "always"],
    "space-in-parens": [0, "never"],
    "strict": 2,
    "valid-typeof": 2,
    "prefer-spread": 0
  }
}